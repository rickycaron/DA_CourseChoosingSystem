put type=\"checkbox\" class=\"custom-control-input\" id=\"editSwitch\" v-model=\"editSwitch\">\r\n            <label class=\"custom-control-label\" for=\"editSwitch\">Edit Courses</label>\r\n        </div>\r\n    </div>\r\n\r\n    \r\n\r\n    <div v-if=\"courses.length\">\r\n        <div class=\"card-deck d-flex justify-content-center\">\r\n            <div v-for=\"course in courses\" :key=\"course.courseID\" >\r\n                <CourseCard :courseinfo=\"course\" :ableToDelete = \"editSwitch\"  @delete-a-Course=\"deleteCourse\">\r\n                </CourseCard>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div v-else>\r\n        <p>Loading page</p>\r\n    </div>\r\n    \r\n\r\n    <CreateCourseForm @create-a-new-course =\"addCourse\">\r\n    </CreateCourseForm>\r\n\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport CourseCard from '../../components/CourseCard.vue'\r\nimport CreateCourseForm from '../../components/CreateCourseForm.vue'\r\n\r\n\r\nexport default {\r\n    components:{CourseCard,CreateCourseForm},\r\n    data(){\r\n        return{\r\n            courses:[],\r\n            editSwitch:false\r\n        }\r\n    },\r\n    mounted() {\r\n        fetch('http://localhost:3000/courses')\r\n        .then((res)=> res.json())\r\n        .then((data) => this.courses = data)\r\n        .catch(err => console.log(err.message)) \r\n\r\n    },\r\n    methods: {\r\n        addCourse(newCourseInfo){\r\n            // console.log(\"I submit!\")\r\n            console.log(newCourseInfo)\r\n            this.courses.push(newCourseInfo)\r\n        },\r\n        deleteCourse(courseid){\r\n            console.log(\"remove course of id\"+ courseid+\"from the list\")\r\n\r\n            // for( let i = 0; i < this.courses.length; i++)\r\n            // {                    \r\n            //     if ( this.courses[i].courseID === courseid)\r\n            //     { \r\n            //         this.courses.splice(i, 1); \r\n            //         i--; \r\n            //     }\r\n            // }\r\n\r\n            let filteredCourses = this.courses.filter(function(course, index, arr)\r\n            { \r\n                return course.courseID != courseid;\r\n            });\r\n            this.courses = filteredCourses\r\n\r\n            console.log(this.courses)\r\n\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}