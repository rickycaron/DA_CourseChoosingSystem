esent(beginTime)}} - {{timePresent(endTime)}}</small>\r\n        </div>\r\n        <small></small>\r\n    </a> -->\r\n    \r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    props: ['courseinfo'],\r\n    data()\r\n    {\r\n        return{\r\n            courseID : this.courseinfo.id,\r\n            courseName : this.courseinfo.courseName,\r\n            courseDetail : this.courseinfo.courseDetail,\r\n            teacherID : this.courseinfo.teacherID,\r\n            teacherName : this.courseinfo.teacherFirstName +' '+ this.courseinfo.teacherLastName,\r\n            // courseImage:require(\"../assets/programminginc.png\"),\r\n            weekDay: this.courseinfo.weekDay,\r\n            beginTime: this.courseinfo.beginTime, \r\n            endTime: this.courseinfo.endTime\r\n        }\r\n    },\r\n    computed:{\r\n        weekDayRepresent()\r\n        {\r\n            let day = ''\r\n            switch (this.weekDay) {\r\n                case 7:\r\n                    day = \"Sun\";\r\n                    break;\r\n                case 1:\r\n                    day = \"Mon\";\r\n                    break;\r\n                case 2:\r\n                    day = \"Tue\";\r\n                    break;\r\n                case 3:\r\n                    day = \"Wed\";\r\n                    break;\r\n                case 4:\r\n                    day = \"Thu\";\r\n                    break;\r\n                case 5:\r\n                    day = \"Fri\";\r\n                    break;\r\n                case 6:\r\n                    day = \"Sat\";\r\n            }\r\n            return day\r\n        }\r\n    },\r\n    methods:{\r\n        timePresent(t)\r\n        {\r\n            let time = t.split(\":\")\r\n            if(time[0] < 12){return (t + \"AM\")}\r\n            else{return (t+\"PM\")}\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}